{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/",
            "redirect": "/ja/",
            "allowedRoles": ["anonymous"],
            "headers": {
                "Cache-Control": "no-store"
            },
            "statusCode": 302
        }
    ],
    "networking": {
        "allowedIpRanges": [
           "AzureFrontDoor.Backend"
        ]
    }
}
